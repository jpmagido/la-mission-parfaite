<div class="container">
  <h1 class="text-center">Tous les Couchages</h1>
  <div>
    <%= render partial: 'housings/search_filter' %>
  </div>
  <div class="row justify-content-center mt-4">
    <% housings.each do |housing| %>
      <div class="col-md-4">
        <%= render partial: 'housings/list', locals: { housing: housing } %>
      </div>
    <% end %>
  </div>
</div>
